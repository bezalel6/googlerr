{
  "name": "googlerr",
  "description": "Calico Colors - A Webview View API Sample",
  "version": "0.0.1",
  "publisher": "bezalel6",
  "license": "MIT",
  "engines": {
    "vscode": "^1.50.0"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:googlerr-sidebar-view",
    "onCommand:googleErr.searchErr",
    "onCommand:googleErr.clear",
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "googlerr-sidebar-view",
          "title": "Googlerr",
          "icon": "media/error.svg"
        }
      ]
    },
    "views": {
      "googlerr-sidebar-view": [
        {
          "type": "webview",
          "id": "googlerr-sidebar-view",
          "name": "Calico Colors"
        }
      ]
    },
    "commands": [
      {
        "command": "googlerr.searchErr",
        "category": "Calico Colors",
        "title": "Search Error"
      },
      {
        "command": "googlerr.clear",
        "category": "Calico Colors",
        "title": "Clear",
        "icon": "$(clear-all)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "googlerr.clear",
          "group": "navigation",
          "when": "view == googlerr-sidebar-view"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jquery": "^3.5.14",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/react": "^18.0.15",
    "@types/vscode": "^1.68.1",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "react": "^18.2.0"
  }
}
