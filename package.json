{
  "name": "googlerr",
  "displayName": "Googlerr",
  "description": "google your way out of the Segmentation faults of the world",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:googlerr.helloWorld",
    "onCommand:googlerr.askQuestion",
    "*",
    "onView:googlerr-sidebar",
    "onCommand:googlerr.searchErr"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "googlerr-sidebar-view",
          "title": "Googlerr",
          "icon": "media/error.svg"
        }
      ]
    },
    "views": {
      "googlerr-sidebar-view": [
        {
          "type": "webview",
          "id": "googlerr-sidebar",
          "name": "Googlerr",
          "icon": "media/error.svg",
          "contextualTitle": "Googlerr"
        }
      ]
    },
    "commands": [
      {
        "command": "googlerr.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "googlerr.askQuestion",
        "category": "Googlerr",
        "title": "Q me!"
      },
      {
        "command": "googlerr.searchErr",
        "title": "Google an error",
        "category": "Googlerr"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "rollup -c && webpack --mode production",
    "testa": "rollup -c",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-sucrase": "^4.0.4",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.20.55",
    "@types/node-fetch": "^2.6.2",
    "@types/polka": "^0.5.1",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "rollup": "^2.3.4",
    "ts-loader": "^8.0.11",
    "typescript": "^4.7.4",
    "vscode-test": "^1.4.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@types/jest": "^28.1.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "polka": "^0.5.2",
    "react": "^18.2.0",
    "url": "^0.11.0"
  }
}
