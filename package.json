{
  "name": "googlerr",
  "displayName": "Googlerr",
  "description": "google your way out of the Segmentation faults of the world",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:googlerr.helloWorld",
    "onCommand:googlerr.askQuestion",
    "*",
    "onView:googlerr-sidebar",
    "googlerr.searchErr"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "googlerr-sidebar-view",
          "title": "Googlerr",
          "icon": "media/error.svg"
        }
      ]
    },
    "views": {
      "googlerr-sidebar-view": [
        {
          "type": "webview",
          "id": "googlerr-sidebar",
          "name": "Googlerr",
          "icon": "media/error.svg",
          "contextualTitle": "Googlerr"
        }
      ]
    },
    "commands": [
      {
        "command": "googlerr.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "googlerr.askQuestion",
        "category": "Googlerr",
        "title": "Q me!"
      },
      {
        "command": "googlerr.searchErr",
        "title": "Google an error",
        "category": "Googlerr"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "rollup -c && webpack --mode production",
    "testa": "rollup -c",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "concurrently": "^7.2.2",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "rollup": "^2.75.7",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.48.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.8.2",
    "typescript": "^4.6.4",
    "vscode-test": "^1.5.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.31",
    "dot-env": "0.0.1",
    "dotenv": "^16.0.1",
    "gulp-sass": "^5.1.0",
    "node-fetch": "^3.2.6",
    "ts-dotenv": "^0.8.3"
  }
}
